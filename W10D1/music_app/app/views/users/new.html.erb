<h2> Sign up here </h2>
  <%=flash[:errors]%>

<form action="<%=users_url%>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%=form_authenticity_token%>"/>
  <label>
    Email:
    <input type="email" name="user[email]"/>
  </label>

  <label>
    Password:
    <input type="password" name="user[password]"/>
  </label>

  </br>

  <input type="submit" value="Sign up now!">
</form>